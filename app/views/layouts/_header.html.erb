<header>
<nav>
<% if user_signed_in?%>
<%=image_tag("icons8-hamburger-32.png", alt: "Hamburger icon")%>
<%= title %>
<%= button_to "Sign Out", destroy_user_session_path, method: :delete, class: 'sign-out' %>
<% else%>
<%= link_to image_tag("icons8-left-arrow-50.png", alt: "left-arrow icon"), root_path %>
<%= title %>
<%= link_to 'Next',root_path, class:"next-link" %>
<% end %>
</nav>
</header>